n=int(input("Enter day of week as integer: "))
if(n==1):
    print("Monday and Not a holiday")
elif(n==2):
    print("Tuesday and not a holiday")
elif(n==3):
    print("Wednesday and not a holiday")
elif(n==4):
    print("Thursday and not a holiday")
elif(n==5):
    print("Friday and not a holiday")
elif(n==6):
    print("Saturday and not a holiday")
elif(n==7):
    print("Sunday and holiday")
else:
    print("Enter correct day of the week")
    
